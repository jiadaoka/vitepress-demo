{
    "name": "@panda-jia/vitepress-demo",
    "version": "0.0.1",
    "keywords": [
        "vitepress",
        "demo",
        "preview",
        "vitepress-demo",
        "shadowDom"
    ],
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && rollup --configPlugin typescript --config build/rollup.config.ts",
        "release": "standard-version --release-as patch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jiadaoka/vitepress-demo"
    },
    "main": "./dist/plugin/index.js",
    "module": "./dist/plugin/index.js",
    "exports": {
        "./plugin": {
            "import": "./dist/plugin/index.js"
        },
        "./component": {
            "import": "./dist/component/index.js"
        }
    },
    "peerDependencies": {
        "vite": "^4.1.1",
        "vue": "^3.2.47"
    },
    "dependencies": {
        "sass": "^1.57.1"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^4.0.3",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/markdown-it": "^12.2.3",
        "@types/node": "^18.11.18",
        "@types/vue": "^2.0.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "cz-customizable": "^7.0.0",
        "rollup": "^3.15.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-polyfill-node": "^0.12.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "typescript": "^4.9.3",
        "vite": "^4.1.2",
        "vue-tsc": "^1.0.11"
    },
    "config": {
        "cz-customizable": {
            "path": "node_modules/cz-customizable",
            "config": ".cz-config.cjs"
        }
    }
}
